<table class='table table-bordered table-hover mt-4'>
  <thead class="thead-dark">
    <tr>
      <th>Symbol</th>
      <th>Company Name</th>
      <th>Price</th>
    </tr>
  </thead>
  <% @stocks.each do |stock| %>
    <tbody>
      <tr>
        <td><%= stock.symbol %></td>
        <td><%= stock.company_name %></td>
        <td>$<%= stock.price %></td>
      </tr>
    </tbody>
  <% end %>
</table>
